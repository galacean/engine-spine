{
  "name": "@ali/ci",
  "version": "4.30.0",
  "description": "自动生成 ci 的 yml 文件",
  "bin": {
    "alicov-report": "bin/alicov-report.js"
  },
  "files": [
    "bin",
    "templates",
    "install.js"
  ],
  "scripts": {
    "env": "node -e 'console.log(process.env)'",
    "test": "NODE_ENV=test ava",
    "cov": "NODE_ENV=test nyc ava --tap | ava-tap-json > node_modules/.tap.json && nyc report --reporter=json --reporter=lcov",
    "ci": "rm -rf node_modules/.tap.json && tnpm run cov",
    "postinstall": "node install.js",
    "autod": "autod -w --prefix='^'",
    "local-install": "CI_ROOT_FOR_TEST=$PWD node install.js",
    "contributors": "ali-contributors"
  },
  "author": {
    "name": "不四",
