    "weburl": "https://code.aone.alibaba-inc.com/node/ci"
  },
  "bugs": {
    "url": "https://gitlab.alibaba-inc.com/egg/egg/issues"
  },
  "dependencies": {
    "@ali/nunjucks": "^5.2.2",
    "alicov": "^2.9.0",
    "rimraf": "^2.6.2",
    "urllib": "^2.34.2"
  },
  "devDependencies": {
    "ali-contributors": "2",
    "autod": "^2.4.2",
    "ava": "^0.16.0",
    "ava-tap-json": "^2.0.4",
    "nyc": "^6.1.1"
  },
  "engines": {
    "node": ">=4.0.0"
  },
